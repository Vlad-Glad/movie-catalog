@{
    ViewData["Title"] = "Import";
}

<h1>Import Movies from Excel</h1>

<h3>Choose Excel file to upload</h3>

@using (Html.BeginForm("Import", "Movies", FormMethod.Post, new { enctype = "multipart/form-data", id = "frm-excel" }))
{
    <div class="mb-3">
        <label for="fileExcel">Upload Excel file:</label>
        <input type="file" name="fileExcel" id="fileExcel" class="form-control" />
    </div>
    <button type="submit" class="btn btn-success">Upload</button>
}
